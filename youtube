

[Script]
youtube_ad = type=http-request,pattern=^https?:\/\/.+?\.googlevideo\.com\/.+&(oad|ctier)=(?!A),script-path=https://choler.github.io/Surge/Script/YouTube.js

[MITM]

hostname = *.googlevideo.com
ctier=[A-Z] url 302 ctier=A
